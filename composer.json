{
    "name": "bluewing/shared-server",
    "type": "library",
    "description": "Shared library for Bluewing products",
    "license":  "proprietary",
    "keywords": ["bluewing", "horizon", "showcrew", "library"],
    "authors": [
        {
            "name": "Luke",
            "email": "lukedavia@icloud.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4",
        "lcobucci/jwt": "^3.3",
        "laravel/framework": "^6.0",
        "stripe/stripe-php": "^7.3",
        "ext-json": "*",
        "myclabs/php-enum": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "classmap": [],
        "psr-4": {
            "Bluewing\\": "src/app/"
        },
        "files": [
            "src/app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "src/tests/"
        }
    },
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Bluewing\\Providers\\BluewingServiceProvider",
                "Bluewing\\Providers\\JsonWebTokenServiceProvider",
                "Bluewing\\Providers\\RefreshTokenServiceProvider",
                "Bluewing\\Providers\\StripeServiceProvider"
            ]
        }
    }
}
